{
    "version": 2,

            "devCommand": "npm run dev",
            "builds": [
              {
                "src": "src/server.ts",
                "use": "@vercel/node"
              }
            ],
            "routes": [
                {
                    "src": "/ping",
                    "dest": "src/api/ping.ts"
                },
              {
                "src": "/(.*)",
                "dest": "src/server.ts"
              }
            ]
    }
    